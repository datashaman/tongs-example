{
  "name": "datashaman/tongs-example",
  "type": "project",
  "description": "Tongs example",
  "keywords": [
    "laravel",
    "ssg",
    "static-site",
    "static-site-generator",
    "tongs"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marlin Forbes",
      "email": "marlinf@datashaman.com"
    }
  ],
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "repositories": {
    "tongs": {
      "type": "path",
      "url": "../tongs",
      "options": {
        "symlink": true
      }
    },
    "tongs-feed": {
      "type": "path",
      "url": "../feed",
      "options": {
        "symlink": true
      }
    },
    "tongs-metadata": {
      "type": "path",
      "url": "../metadata",
      "options": {
        "symlink": true
      }
    },
    "tongs-more": {
      "type": "path",
      "url": "../more",
      "options": {
        "symlink": true
      }
    },
    "tongs-permalinks": {
      "type": "path",
      "url": "../permalinks",
      "options": {
        "symlink": true
      }
    },
    "tongs-sass": {
      "type": "path",
      "url": "../sass",
      "options": {
        "symlink": true
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "datashaman/tongs": "1.*.*",
    "datashaman/tongs-feed": "@dev",
    "datashaman/tongs-metadata": "@dev",
    "datashaman/tongs-more": "@dev",
    "datashaman/tongs-permalinks": "@dev",
    "datashaman/tongs-sass": "@dev"
  }
}
